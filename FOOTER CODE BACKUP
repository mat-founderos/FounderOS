<!-- Auto Update the Copyright Year -->
<script>
		let COPYRIGHT_YEAR = document.querySelector('#copyright-year');
    if (COPYRIGHT_YEAR) COPYRIGHT_YEAR.innerText = new Date().getFullYear();
</script>


<script>
  Webflow.push(function() {
    $('#wf-form-Sign-Up-Form-4').submit(function() {
      let current_redirect_ = jQuery('#wf-form-Sign-Up-Form-4').attr('redirect');
      let survay_redirect = ((current_redirect_).split('?'))[0];
      setTimeout(function() {
        location.href = survay_redirect+'?email=' + $('#Email-Address-9').val();
      }, 100);
    });
  });
  Webflow.push(function() {
    $('.footer #wf-form-Sign-Up-Form-3').submit(function() {
      let current_redirect_ = jQuery('.footer #wf-form-Sign-Up-Form-3').attr('redirect');
      let survay_redirect = ((current_redirect_).split('?'))[0];
      setTimeout(function() {
        location.href = survay_redirect+'?email=' + $('#Email-Address-3').val();
      }, 100);
    });
  });
</script>

